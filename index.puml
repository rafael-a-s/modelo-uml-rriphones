@startuml

'=============== Produto ============'
class Iphone{
  Integer: id
  Double: preco
  String: descricao
}
class Modelo{
  Integer: id
  String: nome
}
enum Cor{
  Integer: id
  String: label
}
class Estoque{
  Integer: id
  Integer: quantidade

}
Iphone "*"--*"1" Cor
Iphone "*"--"1" Modelo
Iphone "1"--"1" Estoque
'=============== Usuario ============'
class Usuario{
  Integer: id
  String: Nome
  String: cpf
  Date: dataNascimento

}
class Telefone{
  Integer: id
  String: codigoDeArea
  String: numero
}
enum Genero{
  Integer: id
  String: label
}
class Endereco{
  Integer: id
  String: cep 
  String: Endereco
  Integer: numero
  String: complemento
}

Usuario "1"--*"1" Genero
Usuario "1"--*"1" Telefone
Usuario "*"--"1" Endereco
'=============== Compra ============'
class Carrinho{
  List<Iphone>: ListaCarrinho
}
class Venda{
  Integer: id
  String: relatorio
}
class Cartao{
  String: numeroCartão
  String: dataVencimento
  String: cvDoCartão
}

Enum OpcaoPagamento{
  Integer: id
  String: label
}

Carrinho "1"--"1" Iphone
Venda "1"--"1" Carrinho
Venda "1"--"*" Usuario
Venda "1"--"1" Cartao
Cartao "1"--*"1" OpcaoPagamento


'=============== relacionamentos ============'


@enduml